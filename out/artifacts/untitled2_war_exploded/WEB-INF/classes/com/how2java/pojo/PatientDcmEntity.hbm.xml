<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.how2java.pojo.PatientDcmEntity" table="patientDCM" schema="patient">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="patientId">
            <column name="patientId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="dcmId">
            <column name="DCMId" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>